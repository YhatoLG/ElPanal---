CREATE TABLE Tipo_De_Documento (
    IdTipo_De_Documento CHAR(36) PRIMARY KEY,
    TipoDocumento VARCHAR(50), -- 'Pasaporte', 'Cedula', 'Tarjeta_De_Identidad', 'Cedula_Extrajeria', 'NIT'
    NumeroDocumento VARCHAR(50), -- NÃºmero del documento
    Activo BIT DEFAULT b'1',
    Actualiza DATETIME DEFAULT CURRENT_TIMESTAMP
);
